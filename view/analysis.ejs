<!DOCTYPE html>
<html lang="en">

<%- include('partial/header')-%>

<body>
    <section id="main">
        <div class="container">
            <div class="row">
                <%- include('partial/sidemenu')-%>
                <div class="col-md-9">
                    <%- include('partial/flash')-%>
                    <div id="status_message"></div>
                    <div class="panel panel-default">
                        <div class="panel-heading main-color-bg">
                            <h3 class="panel-title"><%= title%></h3>
                        </div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-md-12">
                                    <input class="form-control" id="tableSearch" type="text" placeholder="搜索">
                                </div>
                            </div>
                            <br>
                            <table class="table table-striped table-hover">
                                <thead>
                                    <tr>
                                        <th></th>
                                        <th>名稱</th>
                                        <th>性別</th>
                                        <th>項目總數</th>
                                        <th>分析日期</th>
                                        <th class="col-md-1 col-lg-1 col-sm-1"></th>
                                    </tr>
                                    <tr>
                                </thead>
                                <tbody id="myTable">
                                    <% if (data.length) { %>
                                    <% for (var i=0; i< data.length; i++) { %>
                                    <tr>
                                        <td><%= i + 1; %></td>
                                        <td><%= data[i].user_name %></td>
                                        <td><%= data[i].user_gender %></td>
                                        <td><%= data[i].total_method %></td>
                                        <td><%= data[i].created_date %></td>
                                        <td><a class="btn btn-danger" href="/api/analysis/<%= data[i].analysis_id%>"">分析報告</a></td>
                                    </tr>
                                    <% } %>
                                    <% } %>
                                </tbody>
                            </table>
                            <%- include('partial/pagination')-%>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <%- include('partial/footer')-%>

</body>

</html>